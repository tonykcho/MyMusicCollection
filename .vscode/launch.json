    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch MusicAPI",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/MusicApi/bin/Debug/net9.0/MusicApi.dll",
            "launchSettingsFilePath": "${workspaceFolder}/MusicApi/Properties/launchSettings.json",
            "preLaunchTask": "Build",
            "postDebugTask": "Compose Down",
            "cwd": "${workspaceFolder}/MusicApi",
            "logging": {
                "moduleLoad": false,
            }
        },
        {
            "name": "Launch MusicUI",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/music-ui",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
        }
    ]
}